CREATE TABLE PARTIDA (PAR_ID INTEGER AUTO_INCREMENT NOT NULL, PAR_VENC INTEGER, JOG_ID_1 INTEGER NOT NULL, JOG_ID_2 INTEGER NOT NULL, PRIMARY KEY (PAR_ID)) ENGINE=InnoDB
CREATE TABLE USUARIO (USR_ID INTEGER AUTO_INCREMENT NOT NULL, USR_EMAIL VARCHAR(255) NOT NULL, USR_LOGIN VARCHAR(255) NOT NULL, USR_NOME VARCHAR(255) NOT NULL, USR_ONLINE TINYINT(1) default 0 NOT NULL, USR_SENHA VARCHAR(255) NOT NULL, PRIMARY KEY (USR_ID)) ENGINE=InnoDB
ALTER TABLE USUARIO ADD CONSTRAINT UNQ_USR_LOGIN UNIQUE (USR_LOGIN)
ALTER TABLE PARTIDA ADD CONSTRAINT FK_PARTIDA_JOG_ID_1 FOREIGN KEY (JOG_ID_1) REFERENCES USUARIO (USR_ID)
ALTER TABLE PARTIDA ADD CONSTRAINT FK_PARTIDA_JOG_ID_2 FOREIGN KEY (JOG_ID_2) REFERENCES USUARIO (USR_ID)
